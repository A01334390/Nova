<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>Servlets.admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>paciente</servlet-name>
        <servlet-class>Servlets.paciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>geriatria</servlet-name>
        <servlet-class>Servlets.geriatria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nutricion</servlet-name>
        <servlet-class>Servlets.nutricion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>social</servlet-name>
        <servlet-class>Servlets.social</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fitbit</servlet-name>
        <servlet-class>Servlets.fitbit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reporte</servlet-name>
        <servlet-class>Servlets.reporte</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>paciente</servlet-name>
        <url-pattern>/paciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>geriatria</servlet-name>
        <url-pattern>/geriatria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nutricion</servlet-name>
        <url-pattern>/nutricion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>social</servlet-name>
        <url-pattern>/social</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fitbit</servlet-name>
        <url-pattern>/fitbit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reporte</servlet-name>
        <url-pattern>/reporte</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
